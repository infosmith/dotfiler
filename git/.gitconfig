[alias]
    cane = commit --amend --no-edit
    branches = for-each-ref --sort=-committerdate --format=\"%(color:blue)%(color:bold)%(refname:short)\n\t%(color:red)%(authordate:relative)\t%(color:white)%(authorname)\" refs/remotes
[commit]
    template = ~/.gitmessage
[core]
	attributesfile = ~/.gitattributes
	autocrlf = input
	editor = nano
