#Bitbucket (default)
  Host bitbucket.org
  HostName bitbucket.org
  User git
  IdentityFile ~/.ssh/bitbucket

#Github (secondary)
  Host github.com
  HostName github.com
  User git
  IdentityFile ~/.ssh/github

#Gitlab (third)
  Host gitlab.com
  HostName gitlab.com
  User git
  IdentityFile ~/.ssh/gitlab

